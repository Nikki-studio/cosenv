            }
            
            // Add new editor
            textEditors.emplace_back();
            textEditors.back().loadFile(entry.path, entry.content);
        } 
        else if (entry.type == "image") {
            mediaPlayer.showImage(entry.path);
        } 
        else if (entry.type == "audio") {
            mediaPlayer.playAudio(entry.path);
        }
