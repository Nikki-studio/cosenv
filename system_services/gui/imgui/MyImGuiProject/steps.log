class MediaPlayer {
public:
    Mix_Music* music = nullptr;
    SDL_Texture* currentTexture = nullptr;
    SDL_Renderer* renderer = nullptr;
    bool isPlaying = false;
    float audioVolume = 1.0f;
    
    void init(SDL_Window* window) {
        renderer = SDL_CreateRenderer(window, -1, SDL_RENDERER_ACCELERATED);
    }
    
    void playAudio(const std::string& path) {
        stop();
        music = Mix_LoadMUS(path.c_str());
        if (music) {
            Mix_VolumeMusic(static_cast<int>(audioVolume * MIX_MAX_VOLUME));
            Mix_PlayMusic(music, 0);
            isPlaying = true;
        }
    }
    
    void showImage(const std::string& path) {
        stop();
        SDL_Surface* surface = IMG_Load(path.c_str());
        if (surface) {
            if (currentTexture) {
                SDL_DestroyTexture(currentTexture);
            }
            currentTexture = SDL_CreateTextureFromSurface(renderer, surface);
            SDL_FreeSurface(surface);
            isPlaying = true;
        }
    }
    
    void stop() {
        if (music) {
            Mix_HaltMusic();
            Mix_FreeMusic(music);
            music = nullptr;
        }
        isPlaying = false;
    }
    
    void render() {
        if (currentTexture) {
            SDL_RenderClear(renderer);
            SDL_RenderCopy(renderer, currentTexture, NULL, NULL);
            SDL_RenderPresent(renderer);
        }
    }
    
    ~MediaPlayer() {
        stop();
        if (currentTexture) {
            SDL_DestroyTexture(currentTexture);
        }
        if (renderer) {
            SDL_DestroyRenderer(renderer);
        }
    }
};

